{
  "rules": {
    ".read": false,
    ".write": false,
    "games": {
      "{gameId}": {
        ".read": "data.child('open').val() == true || data.child('members').child(auth.uid).val() == true",
        "open": {
          ".read": true,
          ".write": "data.parent().child('owner').val() == auth.uid"
        }
      }
    }
  }
}